{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": [ "index.js", "lib/", "README.md" ],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/jsdoc/"
  }
}